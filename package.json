{
  "name": "tenant-management-system",
  "version": "1.0.0",
  "description": "A tenant management system built with Electron and Vue",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently \"cross-env NODE_ENV=development vite\" \"cross-env NODE_ENV=development electron .\"",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "cross-env NODE_ENV=development electron .",
    "electron:build": "vite build && electron-builder --config electron-builder.json",
    "start": "electron .",
    "test": "jest",
    "test:watch": "jest --watch",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.3.1",
    "echarts": "^5.6.0",
    "electron-log": "^5.4.0",
    "electron-updater": "^6.6.2",
    "element-plus": "^2.4.4",
    "express": "^5.1.0",
    "helmet": "^7.1.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.1",
    "pinia": "^2.1.7",
    "sqlite3": "^5.0.11",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@vitejs/plugin-vue": "^4.5.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.3.3",
    "electron-builder": "^24.9.1",
    "electron-rebuild": "^3.2.9",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "rollup-plugin-visualizer": "^6.0.0",
    "sass": "^1.66.1",
    "terser": "^5.40.0",
    "ts-jest": "^29.1.1",
    "vite": "^4.4.9",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^0.34.4"
  },
  "author": "",
  "license": "ISC"
}
