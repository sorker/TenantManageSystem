{
  "name": "tenant-management-system",
  "version": "1.0.0",
  "description": "A tenant management system built with Electron and Vue",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently \"cross-env NODE_ENV=development vite\" \"cross-env NODE_ENV=development electron .\"",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "cross-env NODE_ENV=development electron .",
    "electron:build": "vite build && electron-builder",
    "start": "electron .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "build": {
    "appId": "com.tenant.management",
    "productName": "租客管理系统",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "src/database/**/*"
    ],
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "echarts": "^5.6.0",
    "element-plus": "^2.4.4",
    "express": "^5.1.0",
    "mysql2": "^3.14.1",
    "pinia": "^2.1.7",
    "sqlite3": "^5.1.7",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@vitejs/plugin-vue": "^4.5.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "vite": "^5.0.10"
  },
  "author": "",
  "license": "ISC"
}
