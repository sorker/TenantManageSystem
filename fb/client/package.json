    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.16.1",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.2",
    "rollup-plugin-visualizer": "^6.0.0",
